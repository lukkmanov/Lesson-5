#include <iostream>

using namespace std;

void shift (int n)
{

    int a[6] = {1,2,3,4,5,6};
         for (int z = 1; z <= n ; z++)
        {
                int tm = a[0];
            for (int i = 0; i < 6 ; i++)
            {
                a[i] = a[i + 1];
            }
                a[5] = tm;
        }
    for (int i = 0 ; i < 6 ; i++)
    {
        cout << a[i] << endl ;
    }
}

int main()
{
    int n ;
    cout << "Enter how many to shift array ";
    cin >> n;
    shift(n);
    return 0;
}
